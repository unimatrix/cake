{
    "name": "unimatrix/cake",
    "description": "Utilities for CakePHP 3.6",
    "type": "cakephp-plugin",
    "homepage": "https://venture.unimatrix.ro",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "cakephp/cakephp": "3.6.*",
        "mtdowling/cron-expression":"^1.2",
        "matthiasmullie/minify": "^1.3",
        "voku/html-min": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Unimatrix\\Cake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unimatrix\\Cake\\Test\\": "tests"
        }
    }
}
